<?page title="Frame1" contentType="text/html;charset=UTF-8"?>

<zk>
    <div id="container1"> 
    <div sclass="scrolling-banner">
    <div sclass="marquee">Rate of Interest for Education Loan in SBI Bank is 5.4% </div></div>
        <grid sclass="form1">
            <auxhead sclass="Header1"> 
                <auxheader colspan="4" iconSclass="z-icon-user-circle-o"><image style="width:40px;height:30px" src="data:image/png;base64,"></image>Registration form</auxheader>
            </auxhead>
            <columns>
                <column/>
                <column/>
            </columns>
            <rows>
                <row id="name">
                    <label value="UserName"/>
                    <textbox id="namebox" hflex="1" constraint="no empty"></textbox>
                </row>
                <row>
                    <label value="EmailID"/>
                    <textbox id="emailbox" hflex="1" constraint="no empty"></textbox>
                </row>
                <row>
                    <label value="PhoneNumber"/>
                    <textbox id="phonebox" hflex="1" constraint="/^\d{10}$/"></textbox>
                </row>
                <row>
                    <label value="Gender"/>
                    <radiogroup id="genderRadio">
                        <image style="width:30px;height:20px;" src = "data:image/png;base64,"></image>
                        <radio label="Male" style="width:30px;height:30px;" value="Male" iconSclass="z-icom-male" checked="true" image=""></radio>
                        <image style="width:30px;height:20px;" src = "data:image/png;base64,"></image>
                        <radio label="Female" style="width:30px;height:30px;" value="Female" iconSclass="z-icom-female"></radio>
                    </radiogroup>
                </row>
                <row>
                    <label value="Date of Birth"/>
                    <datebox id="dob" hflex="1" constraint="no empty, no today,no future"></datebox>
                </row>
                <row>
                    <label value="Password"/>
                    <textbox id="passwordbox" hflex="1" constraint="no empty"></textbox>
                </row>
                <row>
                    <label value="Confirm Password"/>
                    <textbox id="confirmpasswordbox" hflex="1" constraint="no empty"></textbox>
                </row>
                <row id="subResButtons" style="align-items:center;justify-content:center;">                    
                    <button id="resetButton">Reset</button>
                    <button id="submitButton" type="submit" disabled="false" label="Submit" onClick='showContent("RegistrationSuccessful.zul")'></button>
                                        
                </row>
            </rows>
        </grid>
    </div>
    <zscript><![CDATA[
         void showContent(String name){
            org.zkoss.zul.Div ssecondDiv = (org.zkoss.zul.Div) page.getFellow("container1");
            ssecondDiv.getChildren().clear();
            ssecondDiv.appendChild(Executions.createComponents(name, null, null));
       }
       ]]>
</zscript>
   
    <style>
        .form1{
            border:#6baccf 2px solid;
        }
        .radio image {
            width: 20px;
            height: 20px; 
            margin-right: 5px; 
        }
        .marquee{
            display: inline-block;
            animation:scrolling 10s linear infinite;
        }
        @keyframes scrolling{
            0%{
                transform:translateX(100%);
            }
            100%{
                transform:translateX(-100%);
            }
        }
        .scrolling-banner{
            white-space: nowrap;
            overflow: hidden;
            position: relative;
            background-color: #5281c7;
            border-radius:5px;
            padding:10px;
            color: white;
        }

    </style>
</zk>
